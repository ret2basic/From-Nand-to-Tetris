/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not (in=sel, out=temp1);
    And (a=temp1, b=a, out=temp2);
    And (a=sel, b=b, out=temp3);
    Or (a=temp2, b=temp3, out=out);
}